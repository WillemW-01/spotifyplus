{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    // "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.d.ts",
    "scripts/features.json"
  ]
}
