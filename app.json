{
  "expo": {
    "name": "Spotify+",
    "slug": "spotifyplus",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo-dark.png",
    "scheme": "spotifyplus",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-colored.png",
      "resizeMode": "cover",
      "backgroundColor": "#e9495f"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-dark.png",
        "backgroundColor": "#000000"
      },
      "package": "com.willemw.spotifyplus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c0327c65-4d69-4454-b6fb-aa06354f332a"
      }
    },
    "owner": "willemw"
  }
}
